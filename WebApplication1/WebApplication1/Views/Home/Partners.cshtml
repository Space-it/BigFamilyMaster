@model IEnumerable<Member>
@{
    ViewBag.Title = "Партнеры BigFamily - Сделай свое заведение лучше!";
    ViewBag.Meta = "Участники BigFamily, которые доверили нами свой бизенес. Партнеры и заведения в городе Запорожье. BigFamily предоставляет широкий спектр услуг, начиная от ресторанного консалтинга, заканчивая ребрендингом заведения. Наша цель объеденить все заведения Запорожья в одну большую Семью для укрепления бизнеса";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Styles.Render("~/Content/partners_css")
<header>

    <img src="/Content/images/mobile_menu.png" alt="Mobile Menu" class="toggle-nav" />

    <nav class="navigation">
        <ul class="menu active">
            <li>
                @Html.ActionLink("Главная", "Index", "Home")
            </li>
            <li>
                @Html.ActionLink("О нас", "About", "Home")
            </li>
            <li>
                @Html.ActionLink("Услуги", "Services", "Home")
            </li>
            <li>
                @Html.ActionLink("Новости", "News", "Home")
            </li>
            <li>
                @Html.ActionLink("Партнеры", "Partners", "Home")
            </li>
            <li class="click">
                <a href="#contacts">Контакты</a>
            </li>
        </ul>
    </nav>

    <div class="socials">
        <p>+38 (066) 355-84-12</p>
        <img src="/Content/images/vk.png" alt="vk" class="vk_button">
    </div>

    <div class="partners_description">
        <h2>Партнеры</h2>
        <p>Люди, которые нам доверяют</p>
    </div>

</header>


<section class="partners">
    @Html.Partial("PartnersList",Model)
</section>

